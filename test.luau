local dt = require("@std/date_time")
local v = dt.now()
print(`time is {v:format'%X'} {v} {typeof(v)}`)
local sb = dt.precise_clock.now()
print(`precise_time is {sb} {typeof(sb)}`)
local date = dt.from_date(8000, 10, 10)
print(`date: {date}`)
print(dt.from_time(10, 10, 10))
local e = dt.seconds(10)
print(dt.now())
print(date:format('%D'))
print(e:format("%T"))
